{
  "name": "frps-auth",
  "version": "0.33.0",
  "description": "frp,frps, frp插件, 基于node.js的服务端校验插件, 用于对客户端权限校验",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.15.1",
    "egg-cors": "^2.2.3",
    "egg-email": "^1.0.2",
    "egg-jwt": "^3.1.7",
    "egg-mongoose": "^3.3.1",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.11.0",
    "egg-validate": "^2.0.2",
    "egg-view-ejs": "^2.0.1",
    "egg-view-nunjucks": "^2.3.0",
    "egg-view-vue": "^1.0.0",
    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "nodemailer": "^6.6.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "cross-env": "^7.0.3",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start  --daemon  --port=7001 --title=frp-manage --ignore-stderr",
    "stop": "egg-scripts stop --title=frp-manage",
    "dev": "cross-env egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "14"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ywymoshi/frp-auth.git"
  },
  "author": "ywymoshi",
  "license": "MIT"
}
